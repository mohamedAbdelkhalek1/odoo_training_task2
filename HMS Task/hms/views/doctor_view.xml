<odoo>
    <data>

        <record id="doctor_tree_view" model="ir.ui.view">
            <field name="name">doctor tree view</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="first_name"/>
                    <field name="last_name"/>
                </tree>
            </field>
        </record>


        <record id="doctor_action" model="ir.actions.act_window">
            <field name="name">doctor action</field>
            <field name="res_model">hms.doctor</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="doctor_menu" name="Doctors" parent="hms_menu_root" action="doctor_action" groups="hms.hms_manager_group"/>
    </data>
</odoo>